{
    "env": {
        "node": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "eslint:recommended", "google"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
    },
    "overrides": [
        {
          "files": ["*.js"],
          "rules": {
            "require-jsdoc": "off",
            "valid-jsdoc": "off",
            "max-len": ["error", {"code": 150}]
          }
        }
      ]
}
